{
  "name": "@nesvel/nestjs-http",
  "version": "1.0.0",
  "description": "Laravel-inspired HTTP client and server module for NestJS with fluent APIs",
  "license": "MIT",
  "author": "Nesvel",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@nesvel/exceptions": "workspace:*",
    "@nesvel/shared": "workspace:*",
    "@types/express": "^5.0.5",
    "axios": "^1.13.2",
    "form-data": "^4.0.4",
    "image-size": "^2.0.2",
    "mime-types": "^3.0.1",
    "qs": "^6.14.0",
    "ts-mixer": "^6.0.4"
  },
  "peerDependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0",
    "express": "^4.18.0 || ^5.0.0",
    "reflect-metadata": "^0.1.13 || ^0.2.0",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.8",
    "@nestjs/core": "^11.1.8",
    "@nestjs/testing": "^11.1.8",
    "@nesvel/eslint-config": "workspace:*",
    "@nesvel/jest-config": "workspace:*",
    "@nesvel/tsup-config": "workspace:*",
    "@nesvel/typescript-config": "workspace:*",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^24.10.0",
    "@types/qs": "^6.14.0",
    "express": "^5.1.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "nestjs",
    "http",
    "http-client",
    "axios",
    "express",
    "request",
    "response",
    "fluent-api",
    "laravel",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nesvel/nesvel.git",
    "directory": "packages/framework/http"
  },
  "bugs": {
    "url": "https://github.com/nesvel/nesvel/issues"
  },
  "homepage": "https://github.com/nesvel/nesvel/tree/main/packages/framework/http#readme"
}
