{
  "name": "@nesvel/nestjs-storage",
  "version": "1.0.0",
  "description": "A comprehensive NestJS module for object storage with pluggable driver support for S3, MinIO, and other storage backends",
  "license": "MIT",
  "author": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest-e2e.config.js",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.927.0",
    "@aws-sdk/s3-request-presigner": "^3.927.0",
    "minio": "^8.0.6"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.0"
  },
  "peerDependenciesMeta": {
    "@aws-sdk/client-s3": {
      "optional": true
    },
    "@aws-sdk/s3-request-presigner": {
      "optional": true
    },
    "minio": {
      "optional": true
    }
  },
  "devDependencies": {
    "@nesvel/eslint-config": "workspace:*",
    "@nesvel/jest-config": "workspace:*",
    "@nesvel/tsup-config": "workspace:*",
    "@nesvel/typescript-config": "workspace:*",
    "@nestjs/common": "^11.1.8",
    "@nestjs/core": "^11.1.8",
    "@types/mime-types": "^3.0.1",
    "@types/multer": "^2.0.0",
    "mime-types": "^3.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "packageManager": "bun@1.3.2",
  "keywords": [
    "nestjs",
    "storage",
    "s3",
    "minio",
    "object-storage",
    "file-upload",
    "cloud-storage"
  ]
}
