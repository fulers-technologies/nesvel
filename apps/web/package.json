{
  "name": "web",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start -p 3000",
    "lint": "eslint --max-warnings 0",
    "lint:fix": "eslint --fix",
    "type-check": "next typegen && tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "validate": "bun run type-check && bun run lint && bun run format:check",
    "clean": "rm -rf .next out dist build node_modules/.cache",
    "preinstall": "npx only-allow bun"
  },
  "dependencies": {
    "@nesvel/ui": "*",
    "next": "^16.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.1.0",
    "zod": "catalog:validation"
  },
  "devDependencies": {
    "@nesvel/eslint-config": "*",
    "@nesvel/typescript-config": "*",
    "@types/node": "^22.15.3",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "eslint": "^9.38.0",
    "typescript": "5.9.2",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
